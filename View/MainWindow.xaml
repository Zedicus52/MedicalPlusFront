<Window x:Class="MedicalPlusFront.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedicalPlusFront.View" 
        xmlns:viewmodel="clr-namespace:MedicalPlusFront.ViewModel" 
        d:DataContext="{d:DesignInstance Type=viewmodel:MainWindowVM}"
        mc:Ignorable="d"             
        Title="MainWindow"
        Height="600" Width="800"
        MinHeight="600" MinWidth="800">
    <Grid>
        <Frame NavigationUIVisibility="Hidden" Content="{Binding SelectedViewModel}"></Frame>
    </Grid>
</Window>
